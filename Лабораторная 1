#include <stdio.h>
#include <math.h>//подключаем библиотеку math.h для выполнения математических операций 

int main()
{
    float x, y; //вводим переменные типа с плавающей точкой 
    
    for (x = -4; x <= 4; x +=0.1) // производим цикл с промежутком от [-4;4] с шагом 0.1
    
    {
        y = pow(sin(4*x),2)-3/pow(x,3)-3; 
        printf( " y = %f\n", y);//выводим ответ 
    }
    
    return 0;
    
}
